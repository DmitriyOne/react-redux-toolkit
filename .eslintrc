{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "prefer-const": "warn",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "quotes": ["warn", "double"],
    "jsx-quotes": ["warn", "prefer-single"],
    "indent": ["warn", "tab"],
    "max-len": ["warn", {"code": 80}],
    "semi": ["warn", "always"],
    "eol-last": ["warn", "always"]
  }
}
